                    <div class="justify-content-md-start mx-5 row gap-4  justify-content-between">
                        {{#each legsLevel_2}}
                        <div class="card col-3 p-2" style="width: 21rem;">
                            <a style="text-decoration: none;color:black;font-weight:bold"
                                href="/video/{{this.videoId}}">
                                <img src="{{this.image}}" class="card-img-top" alt="image">
                            </a>
                            <div class="card-body">
                                <a style="text-decoration: none;color:black;font-weight:bold"
                                    href="/video/{{this.videoId}}">
                                    <p class="card-text">{{this.title}}</p>
                                </a>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>